slack.slackChannel = "jenkins_notifications"
prettyNode("uber_ios") {
	fastlane.clean()
	fastlane.install_certs()
	fastlane.install_dependencies()
	mobileBuildStage("Shake Shack") {
		fastlane.deploy_jenkins 'PF_D'
	}
	testStage {
		fastlane.perform_tests 'PF_Staging_D'
		archiveAppForTesting 'build/Build/Products/Debug-iphonesimulator/'
	}
	fastlane.run_reports()
}